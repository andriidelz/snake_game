{
  "expo": {
    "name": "Snake Game",
    "slug": "snake-game",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#064e3b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.snakegame.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#064e3b"
      },
      "package": "com.snakegame.mobile"
    },
    "web": {
      "favicon": "./assets/icons/favicon.png"
    },    
    "extra": {
      "apiUrl": "http://192.168.0.148:8080/api",
      "wsUrl": "ws://192.168.0.148:8080/api/multiplayer/ws",
      "eas": {
        "projectId": "твій-eas-project-id"
      }
    }
  }
}